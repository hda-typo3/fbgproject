<ProjectStructur>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.sheet</sheetTitle>
                </TCEforms>
                <type>array</type>
                
                <el>
                    <settings.pages>
                        <TCEforms>
                            <exclude>0</exclude>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.startingpoint</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>10</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.pages>

                    <settings.degreecourse>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.degreecourse</label>
                            <config>
                                <renderType>selectSingle</renderType>
                                <type>select</type>
                                <foreign_table>tx_fbgproject_domain_model_degreecourse</foreign_table>
                                <foreign_table_where>AND tx_fbgproject_domain_model_degreecourse.sys_language_uid=0 ORDER BY tx_fbgproject_domain_model_degreecourse.title</foreign_table_where>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">---</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>   
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>3</size>
                                <allowNonIdValues>1</allowNonIdValues>
                            </config>
                        </TCEforms>
                    </settings.degreecourse>  
                    
                    
                    <settings.category>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.categories</label>
                            <config>
                                <renderType>selectSingle</renderType>
                                <type>select</type>
                                <foreign_table>tx_fbgproject_domain_model_category</foreign_table>
                                <foreign_table_where>AND tx_fbgproject_domain_model_category.sys_language_uid=0 ORDER BY tx_fbgproject_domain_model_category.title</foreign_table_where>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">---</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>   
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>5</size>
                                <allowNonIdValues>1</allowNonIdValues>
                            </config>
                        </TCEforms>
                    </settings.category>  
                    
                    <settings.semester>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.semester</label>
                            <config>
                                <renderType>selectSingle</renderType>
                                <type>select</type>
                                <foreign_table>tx_fbgproject_domain_model_semester</foreign_table>
                                <foreign_table_where>AND tx_fbgproject_domain_model_semester.sys_language_uid=0 ORDER BY tx_fbgproject_domain_model_semester.title</foreign_table_where>
                                 <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">---</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>5</size>
                                <allowNonIdValues>1</allowNonIdValues>
                            </config>
                        </TCEforms>
                    </settings.semester>    
                        
                    <settings.individualprojects>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.individualprojects</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>Hda\Fbgproject\Service\FlexFormService->projects</itemsProcFunc>
                                <renderType>selectMultipleSideBySide</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">---</numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
                                <size>10</size>
                                <multiple>0</multiple>
                                <maxitems>50</maxitems>
                                <enableMultiSelectFilterTextfield>TRUE</enableMultiSelectFilterTextfield>
                            </config>
                        </TCEforms>
                    </settings.individualprojects>
                       
                    <settings.target>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.detailpage </label>
                               <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>3</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.target>
                    
                     <settings.degreecourseShow>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.degreecourseShow</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:label.no</numIndex>
                                        <numIndex index="1">false</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:label.yes</numIndex>
                                        <numIndex index="1">true</numIndex>
                                    </numIndex>
                                </items>
                                <default>false</default>
                            </config>
                        </TCEforms>
                    </settings.degreecourseShow>
                    
                     <settings.studentShow>
                        <TCEforms>
                            <label>LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:fbgproject.studentShow</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:label.no</numIndex>
                                        <numIndex index="1">false</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:fbgproject/Resources/Private/Language/locallang_db.xlf:label.yes</numIndex>
                                        <numIndex index="1">true</numIndex>
                                    </numIndex>
                                </items>
                                <default>true</default>
                            </config>
                        </TCEforms>
                    </settings.studentShow>

                </el>          
            </ROOT>
        </sDEF>
    </sheets>
</ProjectStructur>